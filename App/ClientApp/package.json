{
  "name": "switcherserver",
  "version": "0.4.0",
  "private": true,
  "homepage": "https://atem.streamlia.com",
  "license": "MIT",
  "dependencies": {
    "@microsoft/signalr": "7.0.2",
    "bootstrap": "5.0.1",
    "browserslist": "4.16.6",
    "qrcode.react": "3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-feather": "2.0.10",
    "react-player": "2.11.2",
    "react-router-dom": "6.8.1",
    "react-toastify": "9.1.1",
    "reactstrap": "9.1.5"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.8.3",
    "@parcel/transformer-webmanifest": "2.8.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@types/react-toastify": "^4.1.0",
    "eslint": "8.27.0",
    "parcel": "2.8.3",
    "process": "^0.11.10",
    "typescript": "4.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "clean": "del /Q /S build\\*.*",
    "startcra": "node scripts/start.js",
    "buildcra": "node scripts/build.js && webpack --progress --mode=production",
    "start": "npx parcel ./public/index.html --dist-dir ./build/ --port 1234 --open",
    "build": "npx parcel build ./public/index.html --dist-dir ./build/ --no-source-maps --no-cache",
    "lint": "eslint ./src/"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
